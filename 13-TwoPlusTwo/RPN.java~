public class RPN{
    private ArrayStack stack = new ArrayStack();
    private int current;
    private ArrayStack compblock = new ArrayStack();
    
    public int Calculate(String s){
	for(int i = 0; i < arg.length();i++){
	    if(s.charAt(i) != ' ')
		stack.push(valueOf(s.charAt(i)));
	}
	while(stack.peek() != "+" && stack.peek() != "-" && stack.peek() != "*" && stack.peek() != "/")
	    compblock.push(stack.pop());
	String tmp = stack.pop()
	    if(tmp == "+"){
		int a = ( (int)compblock.pop() + (int)compblock.pop() );
		compblock.push(x);
	    }else if(tmp == "-"){
		int a = ( ( (int)stack.pop() ) + ( ( (int)stack.pop() ) * -1 ) );
	    }else if(tmp == "*")
		a += ( (int)stack.pop  )
	    }
	    
    	    

    }

